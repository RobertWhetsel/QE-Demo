{
  "development": {
    "PORT": 8080,
    "NODE_ENV": "development",
    "ENABLE_LOGGING": true,
    "LOG_LEVEL": "debug",
    "CSV_PATH": "src/models/data/users.csv",
    "SESSION_SECRET": "dev-secret-key",
    "STORAGE": {
      "TYPE": "session",
      "KEYS": {
        "USER_DATA": "appData",
        "AUTH": "isAuthenticated",
        "USER_ROLE": "userRole",
        "USERNAME": "username"
      }
    }
  },
  "production": {
    "PORT": "${PORT}",
    "NODE_ENV": "production",
    "ENABLE_LOGGING": true,
    "LOG_LEVEL": "error",
    "CSV_PATH": "src/models/data/users.csv",
    "SESSION_SECRET": "${SESSION_SECRET}",
    "STORAGE": {
      "TYPE": "session",
      "KEYS": {
        "USER_DATA": "appData",
        "AUTH": "isAuthenticated",
        "USER_ROLE": "userRole",
        "USERNAME": "username"
      }
    }
  }
}
